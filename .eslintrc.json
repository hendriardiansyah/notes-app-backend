{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
    "no-console": 0,
    "quotes": 0,
    "no-underscore-dangle": 0,
    "linebreak-style": 0,
    "import/no-extraneous-dependencies": 0, 
    "class-methods-use-this": 0,
    "no-plusplus": 0,
    "indent": [2, 2, "tab"]
    }
}
